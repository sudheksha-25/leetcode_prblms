class Node{
    int data;
    Node next;
public Node(int data){
    this.data=data;
    this.next=null;
}
}
public class Main{
    public static void printlist(Node n){
        while(n !=null){
            System.out.print(n.data+" ");
            n=n.next;
    }
    }
    public static void main (String[] args){
        Node head=new Node(1);
        Node s=new Node(2);
        Node t=new Node(3);
        
        head.next=s;
        s.next=t;
        printlist(head);
    }
}
